{
  "name": "sr79-main",
  "version": "0.0.0",
  "description": "Main Personal Server",
  "license": "ISC",
  "author": "Kunal Sharma",
  "main": "main.ts",
  "scripts": {
    "build:main": "tsc",
    "build:swish": "rm -rf ./src/dist/ && webpack --config webpack.config.js --mode development --watch",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "start": "npm run build && node src/main.js",
    "prod": "NODE_ENV=production npm run build:prod && node dist/main.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@types/node": "^24.9.1",
    "@types/react": "19.2.2",
    "babel-loader": "^10.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.10",
    "react-compiler-webpack": "^0.2.1",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
